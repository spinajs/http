  },
  "homepage": "https://github.com/spinajs/http#readme",
  "dependencies": {
    "@spinajs/di": "^1.0.11",
    "@spinajs/intl": "^1.0.0",
    "ajv": "^6.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "helmet": "^3.23.0",
    "lodash": "^4.17.14",
    "pug": "^3.0.0",
    "randomstring": "^1.1.5"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/pug": "^2.0.4",
    "@types/randomstring": "^1.1.6",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.1",
    "tslint-circular-dependencies": "^0.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typedoc": "^0.14.2",
    "typescript": "^3.7.3"
  }
